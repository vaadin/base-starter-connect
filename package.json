{
  "name": "base-starter-connect",
  "version": "0.1.1",
  "description": "Vaadin Connect starter project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaadin/base-starter-connect.git"
  },
  "keywords": [
    "Vaadin",
    "Connect",
    "Starter",
    "Base"
  ],
  "scripts": {
    "start:backend": "node scripts/start/backend.js",
    "start:frontend": "node scripts/start/frontend.js",
    "start:apibrowser": "node scripts/start/apibrowser.js",
    "start": "node scripts/start/backend.js -- node scripts/start/apibrowser.js -- node scripts/start/frontend.js",
    "test:unit": "intern environments=node functionalSuites=",
    "test:e2e": "intern",
    "test:frontend": "npm run test:unit && npm run start:frontend -- -- intern",
    "test": "npm run test:unit && npm run start -- -- intern"
  },
  "dependencies": {
    "@vaadin/connect": "^0.1.1",
    "@vaadin/vaadin-button": "^2.1.0",
    "@vaadin/vaadin-text-field": "^2.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.19.0",
    "intern": "^4.3.3",
    "polyserve": "^0.27.13",
    "proxyquire": "^2.1.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0",
    "swagger-ui-dist": "^3.20.2"
  },
  "author": "Vaadin Ltd",
  "license": "Apache-2.0",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  }
}
