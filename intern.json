{
  "functionalSuites": "e2e/**/*.spec.js",
  "serverUrl": "http://localhost:8081",
  "environments": [
    {
      "browserName": "chrome",
      "fixSessionCapabilities": false
    }
  ],
  "browser": {
    "loader": {
      "script": "esm"
    }
  },
  "node": {
    "suites": "frontend/test/**/*.spec.js",
    "plugins": [
      "./frontend/test/utils.js"
    ]
  },
  "instrumenterOptions": {
    "esModules": true
  }
}
