{
  "suites": "frontend/test/**/*.spec.js",
  "functionalSuites": "e2e/**/*.spec.js",
  "serverUrl": "http://localhost:8081",
  "environments": [
    {
      "browserName": "chrome",
      "fixSessionCapabilities": false
    }
  ],
  "environments": [
    "node"
  ],
  "browser": {
    "loader": {
      "script": "esm"
    }
  },
  "node": {
    "plugins": ["@babel/register"]
  },
  "instrumenterOptions": {
    "esModules": true
  }
}
