{
  "functionalSuites": "e2e/**/*.spec.js",
  "serverUrl": "http://localhost:8081",
  "serverPort": 9002,
  "socketPort": 9003,
  "environments": [
    {
      "browserName": "chrome",
      "fixSessionCapabilities": false
    }
  ],
  "browser": {
    "loader": {
      "script": "esm"
    }
  },
  "node": {
    "suites": "frontend/test/**/*.spec.js",
    "plugins": [
      "./frontend/test/utils.js"
    ]
  },
  "instrumenterOptions": {
    "esModules": true
  },
  "configs": {
    "debug-chrome": {
      "environments": {
        "browserName": "chrome",
        "fixSessionCapabilities": false,
        "goog:chromeOptions": {
          "debuggerAddress": "127.0.0.1:9222"
        }
      }
    }
  }
}
